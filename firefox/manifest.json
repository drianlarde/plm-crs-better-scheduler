{
    "manifest_version": 2,
    "name": "Datepicker Test",
    "version": "1.0",
    "description": "A simple datepicker.",
    "icons": {
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["app/content.js"]
      }
    ],
    "browser_action": {
        "default_popup": "app/popup.html"
    }
}
  